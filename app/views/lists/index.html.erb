  <div class= "navbar-lewon mb-5 p-3"style="background-image: url('https://res.cloudinary.com/dohljjbnh/image/upload/v1763745482/1200x680_ywzfk3.jpg');">
<h1>My lists</h1>
  </div>
<div class="container">
<ul class="cards-lists">
  <%@lists.each do |list|  %>
    <% if list.photo.attached? %>
      <div class="card-category mb-5" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path list.photo.key, height: 300, width: 400, crop: :fill %>)">
        <%=link_to list.name, list_path(list) %>
      </div>
    <% end %>
  <% end %>
</ul>
  <%=link_to "add a list", new_list_path,class: "btn btn-primary rounded"%>
</div>
