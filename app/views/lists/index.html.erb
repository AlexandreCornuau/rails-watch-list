<div class="container">
  <div class= "navbar-lewagon">

<h1>My lists</h1>
  </div>







<ul>
  <%@lists.each do |list|  %>
    <% if list.photo.attached? %>
      <div class="card-category mb-5" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path list.photo.key, height: 300, width: 400, crop: :fill %>)">
        <%=link_to list.name, list_path(list) %>
      </div>
    <% end %>
  <% end %>
  <%=link_to "add a list", new_list_path%>
</ul>
</div>
